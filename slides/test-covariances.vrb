\frametitle{Visualization}
\protect\hypertarget{visualization}{}
\begin{Shaded}
\begin{Highlighting}[]
\CommentTok{\# You can also visualize the covariances{-}{-}{-}{-}}
\KeywordTok{library}\NormalTok{(heplots)}
\NormalTok{rate <{-}}\StringTok{ }\KeywordTok{gl}\NormalTok{(K, }\DecValTok{10}\NormalTok{, }\DataTypeTok{labels =} \KeywordTok{c}\NormalTok{(}\StringTok{"Low"}\NormalTok{, }\StringTok{"High"}\NormalTok{))}
\NormalTok{boxm\_res <{-}}\StringTok{ }\KeywordTok{boxM}\NormalTok{(Y, rate)}

\CommentTok{\# You can plot the log generalized variances}
\CommentTok{\# The plot function adds 95\% CI}
\KeywordTok{plot}\NormalTok{(boxm\_res)}
\end{Highlighting}
\end{Shaded}

